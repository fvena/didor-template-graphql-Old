{
  "name": "didor-templates-graphql",
  "version": "0.0.1",
  "description": "Node/Express template for building GraphQL APIs",
  "homepage": "https://github.com/fvena/didor-templates-graphql#readme",
  "author": "Francisco Vena <fvena32@gmail.com>",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=v8.6.0",
    "npm": ">=5.6.0"
  },
  "scripts": {
    "start": "nodemon src/server.js --watch src -r dotenv/config --exec babel-node",
    "dev": "npm-run-all --parallel start playground",
    "playground": "graphql playground",
    "prisma": "prisma",
    "test": "npm run lint && npm run jest",
    "lint": "eslint src/**",
    "jest": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fvena/didor-templates-graphql.git"
  },
  "bugs": {
    "url": "https://github.com/fvena/didor-templates-graphql/issues"
  },
  "keywords": [
    "didor",
    "node",
    "express",
    "apollo",
    "prisma",
    "graphql",
    "api",
    "starter"
  ],
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  },
  "dependencies": {
    "graphql-depth-limit": "^1.1.0",
    "graphql-yoga": "1.8.5",
    "merge-graphql-schemas": "^1.5.1",
    "prisma-binding": "1.5.17",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "graphql-cli": "2.15.9",
    "graphql-tester": "^0.0.5",
    "jest": "^22.4.3",
    "nodemon": "^1.17.3",
    "npm": "^5.8.0",
    "npm-run-all": "4.1.2",
    "prisma": "1.6.3"
  }
