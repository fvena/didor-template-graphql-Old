{
  "name": "didor-templates-graphql",
  "version": "0.0.1",
  "description": "Node/Express template for building GraphQL APIs",
  "homepage": "https://github.com/fvena/didor-templates-graphql#readme",
  "author": "Francisco Vena <fvena32@gmail.com>",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=v8.6.0",
    "npm": ">=5.6.0"
  },
  "scripts": {
    "server": "nodemon src/server.js --watch src --exec babel-node",
    "playground": "wait-on http://localhost:4000/ && prisma playground",
    "dev": "NODE_ENV=development npm-run-all -p server playground",
    "pretest": "npm run test:prisma",
    "test": "NODE_ENV=test npm-run-all -p -r server test:jest",
    "test:jest": "wait-on http://localhost:4001/ && jest src/**",
    "test:prisma": "prisma deploy -e .env.test && prisma reset -fe .env.test",
    "lint": "eslint src/**",
    "validate": "npm run lint && npm run test",
    "precommit": "yarn test",
    "prebuild": "npm run test",
    "build": "rimraf build && babel ./src --out-dir=./build/src --source-maps inline --copy-files",
    "postbuild": "copyfiles .*.yml build",
    "deploy": "prisma deploy && graphql get-schema --project prisma && graphql prepare",
    "preseed": "prisma reset -f && prisma deploy -f",
    "seed": "NODE_ENV=development babel-node src/utils/seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fvena/didor-templates-graphql.git"
  },
  "bugs": {
    "url": "https://github.com/fvena/didor-templates-graphql/issues"
  },
  "keywords": [
    "didor",
    "node",
    "express",
    "apollo",
    "prisma",
    "graphql",
    "api",
    "starter"
  ],
  "dependencies": {
    "dotenv": "^5.0.1",
    "faker": "^4.1.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-yoga": "^1.9.2",
    "merge-graphql-schemas": "^1.5.1",
    "prisma-binding": "^1.5.18",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "copyfiles": "^2.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "graphql-cli": "^2.15.11",
    "graphql-tester": "^0.0.5",
    "jest": "^22.4.3",
    "nodemon": "^1.17.3",
    "npm": "^6.0.0",
    "npm-run-all": "^4.1.2",
    "prisma": "^1.7.1",
    "wait-on": "^2.1.0"
  }
}
